<!DOCTYPE html>
<html lang="en">

<head>
  <% include shared/head %>
</head>

<body>
  <% include shared/navbar %>

  <header id="indexHeader" class="jumbotron header">
    <div class="container">
      <h1>My Website</h1>
    </div>
  </header>
  <section class="py-3">
    <div class="container">
      <h1>商品列表</h1>
      <div class="row">
        <% products.forEach(product => { %>
        <div class="col-md-4">
          <%= product.name %>
          <%= product.price %>
        </div>
        <% }) %>
      </div>
    </div>
  </section>
  <% include shared/scripts %>
</body>

</html>